{
  "name": "@bijan-massoumi/paco-protocol",
  "version": "0.1.3",
  "description": "Hardhat types for the paco protocol",
  "scripts": {
    "test": "forge test --fork-url $RPC -vv ",
    "build": "forge compile --force ; typechain --target=ethers-v5 --out-dir ./typechain ./out/*Example.sol/*.json",
    "startNode": "anvil -m 'test test test test test test test test test test test junk' --chain-id 1337",
    "deployLocal": "forge script script/deploylocalPacoExample.s.sol --rpc-url http://127.0.0.1:8545 --broadcast --skip-simulation"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "files":[
    "typechain",
    "src/*"
  ],
  "dependencies": {
    "@opensea/seaport-js": "^1.0.8",
    "ethers": "^5.0.0",
    "solc": "0.8.18",
    "tsx": "^3.12.3"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.2.0",
    "@types/node": "^18.14.1",
    "typechain": "^8.1.1"
  }
}
